{
  "manifest_version": 2,
  "name": "Yangguang - CSS",
  "version": "1.0.0",
  "description": "My extension to edit CSS",
  "permissions": ["https://*/*", "http://*/*"],
  "content_scripts": [
    {
      "matches": ["*://www.1point3acres.com/*"],
      "css": ["css/1point3acers.css"]
    },
    {
      "matches": ["*://nba.hupu.com/*"],
      "css": ["css/hupu.css"]
    },
    {
      "matches": ["*://bbs.hupu.com/*"],
      "css": ["css/hupubbs.css"]
    },
    {
      "matches": ["*://www.iplaysoft.com/*"],
      "css": ["css/iplaysoft.css"]
    },
    {
      "matches": ["*://dl.iplaysoft.com/*"],
      "css": ["css/iplaysoftdl.css"]
    },
    {
      "matches": ["*://www.youtube.com/*"],
      "css": ["css/youtube.css"]
    },
    {
      "matches": ["*://mail.google.com/*"],
      "css": ["css/gmail.css"]
    }
  ]
}